{
  "name": "relayreactnative",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "start:server": "ts-node-dev --transpile-only --project=./server/tsconfig.json ./server/index.ts",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "relay": "relay-compiler --language typescript --extensions ts tsx --src ./src --schema=./server/star-wars-schema.graphql"
  },
  "dependencies": {
    "apollo-server": "^2.25.0",
    "nexus": "^1.0.0",
    "react": "17.0.1",
    "react-native": "0.64.1",
    "react-native-flipper": "^0.91.2",
    "react-native-flipper-relay-devtools": "^1.0.1",
    "react-relay": "^11.0.2",
    "relay-config": "^11.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@types/jest": "^26.0.23",
    "@types/react-native": "^0.64.5",
    "@types/react-relay": "^11.0.1",
    "@types/react-test-renderer": "^16.9.2",
    "babel-jest": "^26.6.3",
    "babel-plugin-relay": "^11.0.2",
    "eslint": "^7.14.0",
    "graphql": "^15.5.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.64.0",
    "react-test-renderer": "17.0.1",
    "relay-compiler": "^11.0.2",
    "relay-compiler-language-typescript": "^14.0.0",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.3.2"
  },
  "resolutions": {
    "@types/react": "^17"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
